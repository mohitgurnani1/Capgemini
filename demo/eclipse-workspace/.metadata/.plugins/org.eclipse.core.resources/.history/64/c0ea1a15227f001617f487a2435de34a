
public class ContractEmployee extends Employee{

	protected double salary;
	String contractorName;
	
	public ContractEmployee(String fname, String lname, double salary, String grade, MyDate joiningmonth) {
		super(fname, lname, salary, grade, joiningmonth);
		// TODO Auto-generated constructor stub
	}

	
	void calculateSalary(int ratePerHr, int noOfHrs){
		salary=ratePerHr*noOfHrs;
	}
	
	
}
