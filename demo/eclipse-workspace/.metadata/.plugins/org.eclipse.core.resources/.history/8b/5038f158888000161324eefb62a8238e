package tp;

public class sampleException {

	static int r;
	public static void main(String[] args){
		
		System.out.println("hi");
		try{
		setValue(5,0);
		}
		catch(Exception e){
			e.printStackTrace();
		//setValue(5,1);
		}
		
		System.out.println("bye "+r);
		
		
	}
	
	static public void setValue(int val,int den){
		r=val/den;
		
	}
	
	
	
}
