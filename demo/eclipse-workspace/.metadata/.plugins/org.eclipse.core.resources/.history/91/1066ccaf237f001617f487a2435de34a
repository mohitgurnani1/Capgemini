
public class ContractEmployee extends Employee{

	String contractorName;
	protected static int count=0;

	public ContractEmployee(String fname, String lname, int i, int j,String grade,MyDate md,String contractorName) {
		super(fname, lname,  grade*md , i, j);
		this.contractorName=contractorName;
		count++;
	}

	public ContractEmployee(String fname, String lname, int i, int j, String grade, MyDate md, String contractorName2) {
		// TODO Auto-generated constructor stub
	}

	public String getContractorName(){
		return contractorName;
	}
	
	
}
