
public class ContractEmployee extends Employee{

	String contractorName;
	protected static int count=0;

	public ContractEmployee(String fname, String lname, String grade, MyDate joiningmonth,int ratePerHr,int noOfHrs,String contractorName) {
		super(fname, lname,  ratePerHr*noOfHrs , grade, joiningmonth);
		this.contractorName=contractorName;
		count++;
	}

	public String getContractorName(){
		return contractorName;
	}
	
	
}
