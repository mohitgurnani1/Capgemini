
public class LinkedList {

	public Node head=null;
	
	public class Node{
		private int data;
		private Node next;
		}
	
	public  void addNode(int val){
		if(head==null)
		{	head=new Node();
			head.data=val;
		}
		else
		{
			Node temp=new Node();
			temp.data=val;
			temp.next=head;
			head=temp;
		}
	}
	
	public void display(Node head){
		while(head!=null)
		{
			System.out.println(head.data);
			head=head.next;
		}
		
	}
	
}
