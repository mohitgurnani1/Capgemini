package tp;

public class sampleException {

	static int r;
	public static void main(String[] args){
		
		System.out.println("hi");
		try{
		setValue(5);
		}
		catch(Exception e){
			e.printStackTrace();
		}
		
		System.out.println("bye");
		
		
	}
	
	static public void setValue(int val){
		r=val/0;
		
	}
	
	
	
}
