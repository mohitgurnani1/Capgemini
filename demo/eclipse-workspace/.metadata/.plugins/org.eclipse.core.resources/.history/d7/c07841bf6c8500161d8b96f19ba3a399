
public class Produce implements Runnable {

	Container c;
	
	public Produce(Container c){
		this.c=c;
	}
	
	
	@Override
	public void run() {
		// TODO Auto-generated method stub
		while(true)
			{
			try {
				Thread.sleep(1000);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
			c.put();
			
			}
	}
	
}
